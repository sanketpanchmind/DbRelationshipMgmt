@model List<Article>


    <h1>Articles</h1>
<table class="table">
    <thead>
    <th>Id</th>
    <th>Name</th>
    <th>Description</th>
    </thead>
    <tbody>
        @foreach (var i in Model)
        {

            <tr>
                <td>@i.Id</td>
                <td>@i.ArticleTitle</td>
                <td>@i.ArticleDescription</td>
              
            </tr>
        }
    </tbody>
</table>